
config wifi 'general'
	option radio1_found '0'
	option radio2_found '0'
	option radio0_found '1'

config wifi-radio 'radio0'
	option mode '11b'
	option channel_width '20'
	option channel '1'
	option multicast_rate '1'
	option tx_rate '1'
	option rts_threshold '2347'
	option cts_threshold '2347'
	option aggregation 'enable'
	option aggregation_frames '32'
	option aggregation_size '50000'
	option aggregation_min_size '32768'
	option tx_chain_mask '1'
	option rx_chain_mask '1'
	option type 'atheros_lsdk_wlan'
	option devname 'wifi0'

